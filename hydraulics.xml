<pyvcp>
	<label>
		<text>"Spindle"</text>
	</label>
	<bar>
	        <halpin>"spindle-speed"</halpin>
	        <max_>5000</max_>
				    </bar>
	<label>
		<text>"Hydraulics"</text>
	</label>
	<led>
		<halpin>"hydraulics-enabled"</halpin> 
	        <size>20</size> 
		<on_color>"green"</on_color>
		<off_color>"black"</off_color>
	</led>
	<button>
		<halpin>"start-hydraulics"</halpin>
		<text>"Start"</text> 
	</button>
	<button>
		<halpin>"atc-extend"</halpin>
		<text>"ATC Extend"</text> 
	</button>
	<button>
		<halpin>"atc-retract"</halpin>
		<text>"ATC Retract"</text> 
	</button>
	<checkbutton>
		<halpin>"atc-release"</halpin>
		<text>"Tool Release M5"</text> 
	</checkbutton>
	<spinbox>
		<halpin>"atc-spinbox"</halpin>
		<min_>-100</min_>
		<max_>100</max_>
		<initval>0</initval>
		<resolution>0.1</resolution>
	   	<format>"3.3f"</format>
		<font>("Arial",30)</font>
	 </spinbox>


</pyvcp>
